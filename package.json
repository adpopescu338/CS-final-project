{
  "type": "commonjs",
  "name": "CS",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"yarn dev:be\" \"yarn dev:fe\"",
    "dev:be": "nodemon",
    "dev:fe": "cd client && yarn start",
    "build": "tsc && cd client && yarn build",
    "start": "nodemon .dist/index.js"
  },
  "dependencies": {
    "@kubernetes/client-node": "^0.18.1",
    "@react-email/html": "^0.0.4",
    "@react-email/render": "^0.0.7",
    "@sendgrid/mail": "^7.7.0",
    "concurrently": "^8.2.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^5.7.0",
    "mysql2": "^3.5.2",
    "nodemon": "^3.0.1",
    "pg": "^8.11.2",
    "typescript": "^5.1.6",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/pg": "^8.10.2",
    "dotenv": "^16.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0"
  },
  "nodemonConfig": {
    "ignore": [
      "client"
    ],
    "ext": ".ts",
    "exec": "ts-node -r tsconfig-paths/register index.ts"
  }
}
